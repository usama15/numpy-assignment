    "from PIL import Image\n",
    "import os\n",
    "\n",
    "size_512 = (20, 200, 200, 3)\n",
    "\n",
    "for f in os.listdir(\".\"):\n",
    "    if f.endswith(\".jpg\"):\n",
    "        i = Image.open(f)\n",
    "        fn, fext = os.path.splitext(f)\n",
    "        \n",
    "        i.thumbnail(size_512)\n",
    "        i.save(\"my pic/{}_200{}\".format(fn, fext))\n"
   ]
